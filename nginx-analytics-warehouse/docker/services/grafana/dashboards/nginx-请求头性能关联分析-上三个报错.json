          "datasource": {
            "type": "grafana-clickhouse-datasource",
            "uid": "${DS_CLICKHOUSE}"
          },
          "queryType": "table",
          "refId": "A",
          "rawSql": "SELECT\n    multiIf(\n        user_agent_string LIKE '%Chrome%' AND user_agent_string NOT LIKE '%Edge%', 'Chrome',\n        user_agent_string LIKE '%Firefox%', 'Firefox',\n        user_agent_string LIKE '%Safari%' AND user_agent_string NOT LIKE '%Chrome%', 'Safari',\n        user_agent_string LIKE '%Edge%', 'Edge',\n        user_agent_string LIKE '%bot%' OR user_agent_string LIKE '%crawler%', 'Bot',\n        'Other'\n    ) as browser_type,\n    count() as request_count,\n    avg(total_request_duration) as avg_duration,\n    quantile(0.95)(total_request_duration) as p95_duration,\n    (countIf(total_request_duration > $slow_threshold) * 100.0 / count()) as slow_rate\nFROM nginx_analytics.dwd_nginx_enriched_v3\nWHERE $__timeFilter(log_time)\n    AND user_agent_string != '' AND user_agent_string != '-'\n    AND total_request_duration > 0\nGROUP BY browser_type\nHAVING count() >= 10\nORDER BY slow_rate DESC"
        }
      ],
      "title": "浏览器性能关联",
      "type": "table"
