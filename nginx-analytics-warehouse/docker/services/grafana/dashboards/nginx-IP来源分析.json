      "targets": [
        {
          "datasource": {
            "type": "grafana-clickhouse-datasource",
            "uid": "${DS_CLICKHOUSE}"
          },
          "queryType": "table",
          "refId": "A",
          "rawSql": "SELECT\n    client_ip,\n    count() as requests,\n    countIf(response_status_code NOT IN ('200', '201', '202', '204', '301', '302', '304')) as errors,\n    round((countIf(response_status_code NOT IN ('200', '201', '202', '204', '301', '302', '304')) * 100.0 / count()), 2) as error_rate_pct,\n    countIf(response_status_code >= '400' AND response_status_code < '500') as client_errors,\n    countIf(response_status_code >= '500') as server_errors,\n    uniq(request_uri_normalized) as unique_urls,\n    countIf(response_status_code = '404') as not_found_errors,\n    CASE\n        WHEN count() > 10000 AND (countIf(response_status_code NOT IN ('200', '201', '202', '204', '301', '302', '304')) * 100.0 / count()) > 50 THEN 'DDoS+恶意攻击'\n        WHEN count() > 10000 THEN 'DDoS攻击'\n        WHEN (countIf(response_status_code NOT IN ('200', '201', '202', '204', '301', '302', '304')) * 100.0 / count()) > 50 THEN '恶意攻击'\n        WHEN countIf(response_status_code = '404') > 100 THEN '目录扫描'\n        WHEN uniq(request_uri_normalized) > 50 THEN '爬虫行为'\n        ELSE '可疑活动'\n    END as threat_type,\n    CASE\n        WHEN count() > 10000 AND (countIf(response_status_code NOT IN ('200', '201', '202', '204', '301', '302', '304')) * 100.0 / count()) > 50 THEN '极高'\n        WHEN count() > 5000 OR (countIf(response_status_code NOT IN ('200', '201', '202', '204', '301', '302', '304')) * 100.0 / count()) > 30 THEN '高'\n        WHEN count() > 1000 OR (countIf(response_status_code NOT IN ('200', '201', '202', '204', '301', '302', '304')) * 100.0 / count()) > 20 THEN '中'\n        ELSE '低'\n    END as risk_level\nFROM nginx_analytics.dwd_nginx_enriched_v3\nWHERE $__timeFilter(log_time)\nGROUP BY client_ip\nHAVING (count() > 1000 OR (countIf(response_status_code NOT IN ('200', '201', '202', '204', '301', '302', '304')) * 100.0 / count()) > 20 OR countIf(response_status_code = '404') > 20)\nORDER BY count() DESC, error_rate_pct DESC\nLIMIT 20"
