          "queryType": "",
          "rawSql": "SELECT \n  CASE \n    WHEN error_rate < 1 AND avg_response_time < 500 THEN 95\n    WHEN error_rate < 2 AND avg_response_time < 1000 THEN 85\n    WHEN error_rate < 5 AND avg_response_time < 2000 THEN 75\n    WHEN error_rate < 10 AND avg_response_time < 5000 THEN 60\n    ELSE 40\n  END as health_score\nFROM (\n  SELECT \n    countIf(response_status_code != '200')*100.0/count(*) as error_rate,\n    avg(total_request_duration) as avg_response_time\n  FROM nginx_analytics.dwd_nginx_enriched_v3 \n  WHERE $__timeFilter(log_time)\n)",
          "refId": "A"
        }
