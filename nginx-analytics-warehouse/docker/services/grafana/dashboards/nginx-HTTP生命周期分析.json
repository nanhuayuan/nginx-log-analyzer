          "rawSql": "SELECT 'unknown' as value , round(avg(upstream_connect_time) * 1000, 2) as \"(ms)\", round(quantile(0.95)(upstream_connect_time) * 1000, 2) as \"P95(ms)\", count() as value2 FROM nginx_analytics.dwd_nginx_enriched_v3 WHERE $__timeFilter(log_time) AND upstream_connect_time > 0 UNION ALL SELECT 'unknown' as value , round(avg(upstream_response_time) * 1000, 2) as \"(ms)\", round(quantile(0.95)(upstream_response_time) * 1000, 2) as \"P95(ms)\", count() as value2 FROM nginx_analytics.dwd_nginx_enriched_v3 WHERE $__timeFilter(log_time) AND upstream_response_time > 0 UNION ALL SELECT 'Nginx' as value , round(avg(total_request_duration - upstream_response_time) * 1000, 2) as \"(ms)\", round(quantile(0.95)(total_request_duration - upstream_response_time) * 1000, 2) as \"P95(ms)\", count() as value2 FROM nginx_analytics.dwd_nginx_enriched_v3 WHERE $__timeFilter(log_time) AND upstream_response_time > 0 AND total_request_duration > upstream_response_time ORDER BY \"(ms)\" DESC"
